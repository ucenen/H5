body {
  background: #f0f0f0;
}
em {
  font-style: normal;
}
.fade-enter-active,
.fade-leave-active {
  transition: opacity 0.3s;
}
.fade-enter,
.fade-leave-active {
  opacity: 0;
}
.loading-container {
  position: fixed;
  background-color: rgba(0,0,0,0.3);
  z-index: 9999;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
}
